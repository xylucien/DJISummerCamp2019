<launch>
    <!--include file="$(find simple_planner)/launch/simulator.launch"/-->

    <node pkg="simple_planner" type="global_planner" name="global_planner" respawn="true" output="screen" >
        <param name="global_frame" value="map"/>
        <param name="plan_frequency" value="50"/>
        <param name="point_num" value="10"/>
        <param name="waypoint_distance" value="0.3"/>
    </node>

    <node pkg="simple_planner" type="local_planner" name="local_planner" respawn="true" output="screen" >
        <param name="global_frame" value="odom"/>
        <param name="plan_frequency" value="50"/>
        <param name="max_speed" value="0.65"/>
        <param name="max_angle_diff" value="45"/>
        <param name="p_coeff" value="0.115"/>
        <param name="goal_tolerance" value="0.1"/>
        <param name="prune_ahead_distance" value="0.3"/>
        <param name="prune_ahead_distance_local" value="0.25"/>
    </node>

        <node pkg="simple_planner" type="pid_planner" name="pid_planner" respawn="true" output="screen" >
        <param name="max_x_speed" value="0.5"/>
        <param name="max_y_speed" value="0.5"/>
        <param name="max_yaw_speed" value="0.4"/>
        <param name="p_x_coeff" value="1"/>
        <param name="p_y_coeff" value="1"/>
        <param name="p_yaw_coeff" value="1"/>
        <param name="plan_frequency" value="10"/>
        <param name="goal_dist_tolerance" value="0.1"/>
        <param name="goal_angle_tolerance" value="0.05"/>
    </node>




</launch>
